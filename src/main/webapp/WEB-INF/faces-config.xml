<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <managed-bean>
        <managed-bean-name>etudiant</managed-bean-name>
        <managed-bean-class>Metier.Etudiant</managed-bean-class>   
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>login</managed-bean-name>
        <managed-bean-class>Controllers.Login</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>nav</managed-bean-name>
        <managed-bean-class>Controllers.NavigationBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>randomStuff</managed-bean-name>
        <managed-bean-class>Beans.RandomStuff</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>register</managed-bean-name>
        <managed-bean-class>Controllers.Register</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>home</managed-bean-name>
        <managed-bean-class>Controllers.Home</managed-bean-class>
       
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    
    <managed-bean>    
        <managed-bean-name>uploadBean</managed-bean-name>
        <managed-bean-class>Beans.UploadBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>    
        <managed-bean-name>messagingBean</managed-bean-name>
        <managed-bean-class>Beans.MessagingBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
</faces-config>
